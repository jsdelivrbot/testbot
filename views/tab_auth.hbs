<div ng-controller="notesController">
  <h1>CONFIRM YOUR IDENTITY</h1>
  <form method="post" action="/teams/tabs/auth">
    <input type="hidden" name="src" id="src" value="{{src_url}}"></input>
    <p
      <label>Please confirm your identity</label>
      <input name="upn" id="upn" size="40" ng-value="context.upn" required readonly />
    </p>
    <p>
      <button type="submit">Confirm</button>
    </p>
  </form>
</div>
